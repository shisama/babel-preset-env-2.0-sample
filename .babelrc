{
  "env": {
    "current": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "node8": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 8
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "node6": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 6
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "node4": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 4
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": ["transform-runtime"]
    },
    "chrome": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "chrome": 60
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "chrome50": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "chrome": 50
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "Firefox": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "firefox": 55
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "Firefox_beta": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "firefox": 56
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "Safari": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "Safari": 11
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": ["transform-runtime"]
    },
    "Edge": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "Edge": 15
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": ["transform-runtime"]
    },
    "IE11": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "ie": 11
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": ["transform-runtime"]
    }
  }
}